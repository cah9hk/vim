1 call plug#begin('~/.vim/plugged')
2 Plug 'scrooloose/nerdtree'
    3 Plug 'morhetz/gruvbox'
4 call plug#end()
    5
    6 map <C-n> :NERDTreeToggle<CR>
    7
    8 set tabstop=4
    9 set shiftwidth=4
    10 set smarttab
    11 set expandtab
    12 set smartindent
    13 set nu
    14 syntax on
    15 set background=dark
    16 let g:gruvbox_italic=1
    17 let g:gruvbox_termcolors=256
    18 let g:gruvbox_contrast_dark='hard'
    19 colorscheme gruvbox
    20
    ~

